{
  "buildCommand": "chmod +x build-all.sh && ./build-all.sh",
  "outputDirectory": "calorie-tracker/dist",
  "installCommand": "echo 'Dependencies installed via build script'",
  "routes": [
    {
      "src": "/blog/(.*)",
      "dest": "/blog/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
